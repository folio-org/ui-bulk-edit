    "jest": "^26.4.2",
    "jest-css-modules": "^2.1.0",
    "jest-junit": "^11.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intl": "^5.7.0",
    "react-query": "^3.6.0",
