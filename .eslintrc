{
  "extends": ["@folio/eslint-config-stripes"],
  "parser": "@babel/eslint-parser",
  "ignorePatterns": ["**/*.mock.js"],
  "rules": {
    "import/prefer-default-export": "off"
  },
  "overrides": [
    {
      "files": [
        "**/*.test.js",
        "**/*.test.jsx",
        "**/*.mock.js"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}
